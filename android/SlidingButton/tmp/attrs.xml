<?xml version="1.0" encoding="utf-8"?>
<resources>
	<!-- These are the standard attributes that make up a complete theme. -->
	<declare-styleable
		name="Theme">
		<!-- ToggleButton style. -->
		<attr
			name="buttonStyleToggle"
			format="reference" />
		<!-- Default disabled alpha for widgets that set enabled/disabled alpha 
			programmatically. -->
		<attr
			name="disabledAlpha"
			format="float" />
	</declare-styleable>
	<declare-styleable
		name="SlidingButton">
		<!-- The text for the button when it is checked. -->
		<attr
			name="textOn"
			format="string" />
		<!-- The text for the button when it is not checked. -->
		<attr
			name="textOff"
			format="string" />
	</declare-styleable>
</resources>